<%- include('../partials/boilerplate') %>
<body>
    <%- include('../partials/header') %>
    <%- include('../partials/sidebar') %>
    <main class="main">
        <div class="section-title"><%= `${user.name}'s Complete Library`%></div>
        <div class="main-content"> 
            <% for (i=0; i < dvdList.length; i++) { %>
                
                <a href="/library/dvd/<%= user.id %>/<%= dvdList[i].id %>"><div><%= `${dvdList[i].name} -- ${dvdList[i].year} -- ${dvdList[i].Imdb.imdbnum}` %></div></a>
            <% } %>
        </div>
    </main>
    
    <%- include('../partials/footer') %>
</body>
</html>